{
  "name": "bikerental-backend",
  "version": "1.0.0",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "node --watch index.js",
    "seed": "node scripts/seedBikes.js",
    "seed:users": "node scripts/seedUsers.js",
    "seed:locations": "node scripts/seedLocations.js",
    "seed:admins": "node scripts/seedAdmins.js",
    "seed:bikes": "node scripts/seedBikesWithLocations.js",
    "seed:all": "node scripts/seedLocations.js && node scripts/seedBikesWithLocations.js && node scripts/seedUsers.js",
    "migrate:bikeTypes": "node scripts/migrateBikeTypes.js"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.948.0",
    "@aws-sdk/s3-request-presigner": "^3.948.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.20.2",
    "multer": "^2.0.2",
    "nodemailer": "^7.0.11",
    "razorpay": "^2.9.6"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.6"
  }
}
